@*@model I_footer *@

<div class="dc-footer">
    <div class="container">
        <div class="dc-footer__wrap">
            <div class="row">
                <div class="col-5">
                    @await Html.PartialAsync("_Logo")
                    @await Html.PartialAsync("_FooterDesc")
                    @await Html.PartialAsync("_SocBtns")
                    @await Html.PartialAsync("_Copyright")
                </div>
                <div class="col">
                    <div class="dc-footer__menu">
                        @await Html.PartialAsync("_FooterMenu")
                        @await Html.PartialAsync("_FooterMenu")
                        @await Html.PartialAsync("_FooterMenu")
                    </div>
                </div>      
            </div>
        </div>
    </div>
</div>
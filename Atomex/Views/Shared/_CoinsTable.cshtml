@*@model I_coins-table *@

<table class="dc-pairs-table">
    <tr>
        <th>Name</th>
        <th>Price @await Html.PartialAsync("_TableOrderIcons")</th>
        <th>Market Cap @await Html.PartialAsync("_TableOrderIcons")</th>
        <th>24h change @await Html.PartialAsync("_TableOrderIcons")</th>
        <th>Availability</th>
    </tr>

    @for (int i = 0; i < 6; i++)
    {
        <tr>
            <td>
                @await Html.PartialAsync("_Coin")
            </td>

            <td><span>≈ $13.10580488</span></td>
            <td><span>$360.9812B</span></td>
            <td><span>-3.46%</span></td>
            <td><span>app</span></td>
        </tr>
    }
</table>